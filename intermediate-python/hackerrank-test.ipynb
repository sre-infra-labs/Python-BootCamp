    "def fizzBuzz(n):\n",
    "    for i in range(1,n+1):\n",
    "        if i%3==0 and i%5==0:\n",
    "            print(f'FizzBuzz')\n",
    "        elif i%3==0:\n",
    "            print(f'Fizz')\n",
    "        elif i%5==0:\n",
    "            print(f'Buzz')\n",
    "        else:\n",
    "            print(f'{i}')\n",
    "\n",
    "fizzBuzz(20)"
   ]
